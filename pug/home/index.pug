extends ../layout/layout.pug
block variable
    -var path = '../';


block content

    // begin firstScreen
    section#firstScreen.firstScreen.section
        .container_center
            .firstScreen__content
                .firstScreen__left
                    h1.firstScreen__title
                        span Eurasian Brands
                    .firstScreen__desc
                        span патентный центр №1 в Казахстане
                .firstScreen__right
                    a(href="#").btn Получить прайс на все услуги
                        span + коммерческое предложение
                    a(href="#").btn.btn_whatsapp.btn_success Спросить у нас в WhatsApp
                        span время ответа 15 секунд
    // end firstScreen

    // begin advantage
    section#advantage.advantage.section
        .advantage__bg(style="background-image: url('../img/advantage_bg.webp')")
            .advantage__lines.desktop
                span
                span
                span
                span
                span
        .advantage__wrap
            .container_center
                .advantage__list
                    each item in [ {title: '3000+<br> клиентов', subtitle: 'Нас выбирает каждый<br> второй бизнес в стране'},  {title: '1358 товарных<br> знаков', subtitle: 'зарегистрировали,<br> 97% с первого раза'},  {title: '100% гарантия<br> результата', subtitle: 'при отказе добиваемся<br> регистрации за свой счет'},  {title: 'Рассрочка<br> до 12 мес', subtitle: 'Оплачивайте услуги<br> без первого взноса'} ]

                        .advantage__item
                            i.icon-check_mark
                            .advantage__title
                                span!=item.title
                            .advantage__subtitle
                                span!=item.subtitle




    // end advantage

    // begin comments
    section#comments.comments.section
        .container_center
            .comments__top

                .comments__content
                    .comments__label
                        span Скорее всего
                    h2.section__title
                        span Вы точно знаете наших <b>клиентов</b>
                    .section__subtitle
                        span За брендами стоят люди. Многие стали нашими хорошими друзьями.

                .comments__slider.swiper.comments-swiper-js
                    .swiper__label.mobile
                        span Передвигайте шаги влево/вправо для просмотра всех этапов работы
                        .swiper__img
                            img(src="../img/hand.svg")
                    .commentsSlider.swiper-wrapper
                        each item in [ { img: '1', name: 'Алагузова<br> Баян', info: 'Продюсер, бизнес-леди, глава компании "Shine Production", телеведущая, актриса, певица', commit: 'Из огромного выбора компаний на рынке Казахстана, мне порекомендовали именно "Eurasian Brands" как одних из лучших профессионалов в сфере регистрации и защиты авторского права. Команда "Eurasian Brands" выступила в роли моих представителей в качестве защитников в судебном процессе по авторским правам на сценарий и фильм "Станция судьбы".'}, { img: '2', name: 'Lorem.', info: 'глава компании "Shine Production"', commit: 'Lorem ipsum dolor sit amet, consectetur adipisicing elit. Nostrum, facilis.'}, { img: '3', name: ' Lorem ipsum.', info: ' Lorem.', commit: ' Lorem ipsum dolor sit amet, consectetur adipisicing elit. Beatae distinctio, rem aperiam porro fugiat dolores facere illum ipsum officia natus commodi aut vel sunt cupiditate consequatur eveniet vero, ea perspiciatis?'} ]
                            .swiper-slide
                                .commentsSlider__item
                                    .commentsSlider__persone
                                        .commentsSlider__foto(style="background-image: url('../img/persone_"+item.img+".png')")
                                            .commentsSlider__play
                                                i.icon-play
                                        .commentsSlider__description
                                            .commentsSlider__name
                                                span!=item.name
                                            .commentsSlider__info
                                                span=item.info


                                    .commentsSlider__content
                                        i.icon-quote_2
                                        span=item.commit
                                        a(href="#") Читать полностью

                .comments__control
                    .comments__arrows.desktop
                        i.icon-drop_left
                        i.icon-drop_right
                    .comments__dotte




            .comments__bottom.swiper.logoSlider-swiper-js
                .swiper__label.mobile
                    span Передвигайте влево/вправо для просмотра логотипов наших клиентов
                    .swiper__img
