                .logoSlider.swiper-wrapper
                    each item in [1,2,3,4,1]
                        .swiper-slide
                            .logoSlider__grid
                                each item in [1,2,3,4]
                                    .logoSlider__item
                                        .logoSlider__img
                                            img(src="../img/comments_logo_"+item+".png")
                .logoSlider__doted


            .comments__button
